<div class="col container">
    <div class="row">
        <div class="col">
            <img width="72px" height="72px" style="display: inline-block; image-rendering: pixelated;" src="../assets/accessory/<%= accessory.id %>.png">
            <h1 style="display: inline-block; vertical-align: middle;" class="fw-bold text-decoration-underline display-3">
                <%= accessory.name_en %>
            </h1>
        </div>
    </div>
    <div class="row">
        <div class="col"><strong>Internal ID:</strong> <%= accessory.id %></div>
    </div>
    <div class="row">
        <div class="col"><strong>Rating:</strong> <%= render("rating", accessory.rarity) %></div>
    </div>
    <div class="row">
        <div class="col"><strong>Price:</strong> <%= accessory.price %> CS</div>
    </div>
    <div class="row">
        <div class="col"><strong>Effect:</strong> <%= accessory.desc_en %></div>
    </div>
</div>
